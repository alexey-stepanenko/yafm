Процедура СформироватьЗначенияПеречислений(Значения)
	Для Каждого Элемент Из Значения.ЗначенияПеречисленийПоЯзыкам Цикл
		ТипПеречисления = Элемент.Ключ;
		ЗначенияПеречисления = Новый Соответствие;
		
		Для Каждого Язык Из Элемент.Значение Цикл
			Для Каждого ЗначенияЯзыка Из Язык.Значение Цикл
				ЗначенияПеречисления.Вставить(ВРег(ЗначенияЯзыка.Ключ), ЗначенияЯзыка.Значение);
			КонецЦикла;
		КонецЦикла;
		
		Значения.Значения.Вставить(ТипПеречисления, ЗначенияПеречисления);
	КонецЦикла;
КонецПроцедуры

Процедура СформироватьПредставленияЗначенийПеречислений(Значения)
	Для Каждого Элемент Из Значения.ЗначенияПеречисленийПоЯзыкам Цикл
		ТипПеречисления = Элемент.Ключ;
		
		Для Каждого Язык Из Элемент.Значение Цикл
			ПредставленияЗначений = Значения.ПредставленияЗначенийПеречисленийПоЯзыкам.Получить(Язык.Ключ);
			Если ПредставленияЗначений = Неопределено Тогда
				ПредставленияЗначений = Новый Соответствие;
			КонецЕсли;
			Для Каждого ЗначенияЯзыка Из Язык.Значение Цикл
				ПредставленияЗначений.Вставить(
					ЗначенияЯзыка.Значение,
					СтрШаблон(
						"%1.%2",
						Значения.ИменаПеречисленийПоТипу.Получить(Элемент.Ключ),
						ЗначенияЯзыка.Ключ
					)
				);
			КонецЦикла;
			Значения.ПредставленияЗначенийПеречисленийПоЯзыкам.Вставить(Язык.Ключ, ПредставленияЗначений);
		КонецЦикла;
	КонецЦикла;
	
	Коллекция = Значения.ПредставленияЗначенийПеречисленийПоЯзыкам.Получить(МФПовтИсп.РусскийЯзык());
	Коллекция.Вставить(Неопределено, "Неопределено");
	Коллекция.Вставить(Истина, "Истина");
	Коллекция.Вставить(Ложь, "Ложь");
		
	Коллекция = Значения.ПредставленияЗначенийПеречисленийПоЯзыкам.Получить(МФПовтИсп.АнглийскийЯзык());
	Коллекция.Вставить(Неопределено, "Undefined");
	Коллекция.Вставить(Истина, "True");
	Коллекция.Вставить(Ложь, "False");
КонецПроцедуры

#КонецОбласти
