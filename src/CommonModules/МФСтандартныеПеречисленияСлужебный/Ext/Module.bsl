	//Результат.Вставить("", Неопределено);
	
	Возврат Результат;
КонецФункции

Процедура ДобавитьСтандартноеПеречисление(СтандартныеПеречисления, ИмяТипа, Значения)
	Исключения = Исключения();
	Если Исключения.Найти(ИмяТипа) = Неопределено Тогда
		ТипПеречисления = Тип(ИмяТипа);
	Иначе
		ТипПеречисления = ИмяТипа;
	КонецЕсли;
	// TODO 2025-04-12: Добавить имя перечисления в зависимости от языка.
	СтандартныеПеречисления.ИменаПеречисленийПоТипу.Вставить(ТипПеречисления, ИмяТипа);
	СтандартныеПеречисления.ЗначенияПеречисленийПоЯзыкам.Вставить(ТипПеречисления, Значения);
КонецПроцедуры

Функция Исключения()
	Результат = Новый Массив;
	
	Результат.Добавить(МФПовтИсп.ТипБулевоНеопределено());
	
	Возврат Результат;
КонецФункции

Процедура ДобавитьЗначениеСтандартногоПеречисления(ЗначенияСтандартногоПеречисления, ПредставлениеЗначения, Значение, Язык)
	ЗначенияПоЯзыку = ЗначенияСтандартногоПеречисления.Получить(Язык);
	Если ЗначенияПоЯзыку = Неопределено Тогда
		ЗначенияПоЯзыку = Новый Соответствие;
	КонецЕсли;
	
	ЗначенияПоЯзыку.Вставить(ПредставлениеЗначения, Значение);
	
	ЗначенияСтандартногоПеречисления.Вставить(Язык, ЗначенияПоЯзыку);
КонецПроцедуры

