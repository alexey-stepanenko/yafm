	// русский
	Результат.Вставить(ВРег("Авто"), ВариантУправленияВысотойТаблицы.Авто);
	Результат.Вставить(ВРег("ВСтрокахТаблицы"), ВариантУправленияВысотойТаблицы.ВСтрокахТаблицы);
	Результат.Вставить(ВРег("ВСтрокахФормы"), ВариантУправленияВысотойТаблицы.ВСтрокахФормы);
	Результат.Вставить(ВРег("ПоСодержимому"), ВариантУправленияВысотойТаблицы.ПоСодержимому);
	
	// английский
	Результат.Вставить(ВРег("Auto"), ВариантУправленияВысотойТаблицы.Авто);
	Результат.Вставить(ВРег("UseHeightInTableRows"), ВариантУправленияВысотойТаблицы.ВСтрокахТаблицы);
	Результат.Вставить(ВРег("UseHeightInFormRows"), ВариантУправленияВысотойТаблицы.ВСтрокахФормы);
	Результат.Вставить(ВРег("UseContentHeight"), ВариантУправленияВысотойТаблицы.ПоСодержимому);
	
	Возврат Результат;
