Функция ЗначенияАвтоИзменениеРегистраПриВводеТекста()
	Результат = Новый Соответствие;
	
	// русский
	Результат.Вставить(ВРег("Авто"), АвтоИзменениеРегистраПриВводеТекста.Авто);
	Результат.Вставить(ВРег("ВсеСимволы"), АвтоИзменениеРегистраПриВводеТекста.ВсеСимволы);
	Результат.Вставить(ВРег("Нет"), АвтоИзменениеРегистраПриВводеТекста.Нет);
	Результат.Вставить(ВРег("Предложения"), АвтоИзменениеРегистраПриВводеТекста.Предложения);
	Результат.Вставить(ВРег("Слова"), АвтоИзменениеРегистраПриВводеТекста.Слова);
	
	// английский
	Результат.Вставить(ВРег("Auto"), АвтоИзменениеРегистраПриВводеТекста.Авто);
	Результат.Вставить(ВРег("AllCharacters"), АвтоИзменениеРегистраПриВводеТекста.ВсеСимволы);
	Результат.Вставить(ВРег("None"), АвтоИзменениеРегистраПриВводеТекста.Нет);
	Результат.Вставить(ВРег("Sentences"), АвтоИзменениеРегистраПриВводеТекста.Предложения);
	Результат.Вставить(ВРег("Words"), АвтоИзменениеРегистраПриВводеТекста.Слова);
	
	Возврат Результат;
КонецФункции

Функция ЗначенияАвтоИсправлениеПриВводеТекста()
	Результат = Новый Соответствие;
	
	// русский
	Результат.Вставить(ВРег("Авто"), АвтоИсправлениеПриВводеТекста.Авто);
	Результат.Вставить(ВРег("Использовать"), АвтоИсправлениеПриВводеТекста.Использовать);
	Результат.Вставить(ВРег("НеИспользовать"), АвтоИсправлениеПриВводеТекста.НеИспользовать);
	
	// английский
	Результат.Вставить(ВРег("Auto"), АвтоИсправлениеПриВводеТекста.Авто);
	Результат.Вставить(ВРег("Use"), АвтоИсправлениеПриВводеТекста.Использовать);
	Результат.Вставить(ВРег("DontUse"), АвтоИсправлениеПриВводеТекста.НеИспользовать);
	
	Возврат Результат;
КонецФункции

Функция ЗначенияБулево()
	Результат = Новый Соответствие;
	
	// русский
	Результат.Вставить(ВРег("Истина"), Истина);
	Результат.Вставить(ВРег("Ложь"), Ложь);
	Результат.Вставить(ВРег("Да"), Истина);
	Результат.Вставить(ВРег("Нет"), Ложь);
	
	// английский
	Результат.Вставить(ВРег("True"), Истина);
	Результат.Вставить(ВРег("False"), Ложь);
	Результат.Вставить(ВРег("Yes"), Истина);
	Результат.Вставить(ВРег("No"), Ложь);
	
	// Числа
	Результат.Вставить("1", Истина);
	Результат.Вставить("0", Ложь);
	
	Возврат Результат;
КонецФункции

Функция ЗначенияБулевоНеопределено()
	Результат = ЗначенияБулево();
	
	// русский
	Результат.Вставить(ВРег("Неопределено"), Неопределено);
	
	// английский
	Результат.Вставить(ВРег("Undefined"), Неопределено);
	
	Возврат Результат;
КонецФункции

Функция ЗначенияВажностьПриОтображении()
	Результат = Новый Соответствие;
	
	// русский
	Результат.Вставить(ВРег("Авто"), ВажностьПриОтображении.Авто);
	Результат.Вставить(ВРег("Высокая"), ВажностьПриОтображении.Высокая);
	Результат.Вставить(ВРег("Низкая"), ВажностьПриОтображении.Низкая);
	Результат.Вставить(ВРег("Обычная"), ВажностьПриОтображении.Обычная);
	Результат.Вставить(ВРег("ОченьВысокая"), ВажностьПриОтображении.ОченьВысокая);
	Результат.Вставить(ВРег("ОченьНизкая"), ВажностьПриОтображении.ОченьНизкая);
	
	// английский
	Результат.Вставить(ВРег("Auto"), ВажностьПриОтображении.Авто);
	Результат.Вставить(ВРег("High"), ВажностьПриОтображении.Высокая);
	Результат.Вставить(ВРег("Low"), ВажностьПриОтображении.Низкая);
	Результат.Вставить(ВРег("Usual"), ВажностьПриОтображении.Обычная);
	Результат.Вставить(ВРег("VeryHigh"), ВажностьПриОтображении.ОченьВысокая);
	Результат.Вставить(ВРег("VeryLow"), ВажностьПриОтображении.ОченьНизкая);
	
	Возврат Результат;
КонецФункции

Функция ЗначенияВариантВыравниванияЭлементовИЗаголовков()
	Результат = Новый Соответствие;
	
	// русский
	Результат.Вставить(ВРег("Авто"), ВариантВыравниванияЭлементовИЗаголовков.Авто);
	Результат.Вставить(ВРег("Нет"), ВариантВыравниванияЭлементовИЗаголовков.Нет);
	Результат.Вставить(ВРег("ЭлементыЛевоЗаголовкиЛево"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыЛевоЗаголовкиЛево);
	Результат.Вставить(ВРег("ЭлементыЛевоЗаголовкиПраво"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыЛевоЗаголовкиПраво);
	Результат.Вставить(ВРег("ЭлементыПравоЗаголовкиЛево"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыПравоЗаголовкиЛево);
	Результат.Вставить(ВРег("ЭлементыПравоЗаголовкиПраво"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыПравоЗаголовкиПраво);
	
	// английский
	Результат.Вставить(ВРег("Auto"), ВариантВыравниванияЭлементовИЗаголовков.Авто);
	Результат.Вставить(ВРег("None"), ВариантВыравниванияЭлементовИЗаголовков.Нет);
	Результат.Вставить(ВРег("ItemsLeftTitlesLeft"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыЛевоЗаголовкиЛево);
	Результат.Вставить(ВРег("ItemsLeftTitlesRight"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыЛевоЗаголовкиПраво);
	Результат.Вставить(ВРег("ItemsRightTitlesLeft"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыПравоЗаголовкиЛево);
	Результат.Вставить(ВРег("ItemsRightTitlesRight"), ВариантВыравниванияЭлементовИЗаголовков.ЭлементыПравоЗаголовкиПраво);
	
	Возврат Результат;
КонецФункции

Функция ЗначенияВариантУправленияВысотойТаблицы()
