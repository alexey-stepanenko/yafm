Функция НовыйЗначения()
	Результат = Новый Структура;
	
	Результат.Вставить("ИменаПеречисленийПоТипу", Новый Соответствие);
	Результат.Вставить("ЗначенияПеречисленийПоЯзыкам", Новый Соответствие);
